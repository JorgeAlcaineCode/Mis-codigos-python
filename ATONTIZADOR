#guardamos una palabra y la editamos
def atontizador():
    i = 0
    palabra= raw_input("Una palabra con muchas vocales")
    for cont in palabra:
        if (cont in "AEIOUaeiou"):
            print ("U")
            i += 1
        else:
            print(palabra[i])
            i += 1
    print(palabra)
atontizador()
