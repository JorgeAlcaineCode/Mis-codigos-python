#vamos a hacer un ejercicio que genere al azar un n√∫mero entre 1 y 6.
import random
import time
def dado():
    semilla=time.time()
    print("semilla= "+str(semilla))
    for cont in range(1,101):
        tirada = int(1+random.random()*10%6)
        print("Ha salido un "+str(tirada))
    print("se acabo el juego")
dado()
