#Este c√≥digo permite borrar caracteres que tu le digas.
def borrador():
    dele = str(input("Que caracteres no quieres dentro de la frase"))
    st = str(input("Dime una frase"))
    result = " "
    for df in st:
        if df in dele:
            result += " "
        else:
            result += str(df)
    print (result)
borrador()
