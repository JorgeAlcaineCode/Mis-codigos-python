#Este c√≥digo permite borrar caracteres que tu le digas.
def borrador():
    dele = raw_input("Que caracteres no quieres dentro de la frase")
    st = raw_input("Dime una frase")
    result = " "
    for df in st:
        if df in dele:
            result += " "
        else:
            result += str(df)
    print (result)
borrador()
