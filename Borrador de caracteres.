#Este código permite borrar caracteres que tu le digas.
def borrador():
    result = " "
    frase = raw_input("Dime una frase")
    borrar = raw_input("¿Que caracteres quieres borrar?")
    for a in frase:
        if a not in borrar:
            result += a
        else:
            result += " "
    print (result)
borrador()
