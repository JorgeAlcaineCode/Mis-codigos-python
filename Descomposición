def Descomposicion():
  result = int(input("Â¿Que numero quieres dividir?"))
  ite = 2
  while result != 0:
    if result % ite == 0:
      result = (result / ite)
      print(" dividido entre "+str(ite))
      print("el resultado de la division es: "+str(result))
    else :
      ite +=(1)
Descomposicion()
