def mayor():
	mayor=0#inicializar la variable.
	for cont in range(1,6):
		numero=int(input("Dime un número entero mayor que cero: "))
		if(numero>mayor):
			mayor=numero
print(str(mayor))

def menor():
	menor= int(input("Dime un número entero mayor que cero:"))
	for cont in range(1,6):
		numero=int(input("Dime un número entero mayor que cero: "))
		if(numero<menor):
			mayor=numero
print(str(menor))
